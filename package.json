{
  "name": "@okdiscord/okbot",
  "version": "3.0.0",
  "repository": "git@github.com:okdiscord/okbot",
  "author": "OKDiscord Dev Team",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@prisma/cli": "^2.15.0",
    "@prisma/client": "^2.21.2",
    "concurrently": "^6.0.2"
  },
  "scripts": {
    "bot:dev": "./bin/bot dev",
    "core:dev": "./bin/core compile:watch",
    "front:dev": "./bin/frontend serve",
    "rest:dev": "./bin/rest dev",
    "build": "./bin/build",
    "dev": "./bin/core compile && concurrently -k -n 'ok:core,ok:bot,ok:rest' -c 'magenta.bold,red.bold,white.bold' 'yarn core:dev' 'yarn bot:dev' 'yarn rest:dev'"
  }
}
